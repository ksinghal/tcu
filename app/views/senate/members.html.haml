=h content_for(:title, "Tufts Community Union - Senate")
.contentsection
  .container-fluid
    - @senate_members.in_groups_of(4, false).each_with_index do |group, index|
      .row-fluid
        .paddedrow
          - group.each do |member|
            .span3
              .memberimgs
                .profileimg
                  = image_tag member.profile_image.url(:large)
                  / = image_tag("http://imgur.com/3E6xW9l.jpg", class: "profileimg")
                .textline1
                  = member.name
                .textline2
                  = mail_to member.email